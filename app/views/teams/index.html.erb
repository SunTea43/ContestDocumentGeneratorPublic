<p style="color: green"><%= notice %></p>

<h1>Teams</h1>

<div id="teams">
  <% @teams.each do |team| %>
    <%= render team %>
    <p>
      <%= link_to "Show this team", team %>
    </p>
  <% end %>
</div>

<%= link_to "New team", new_team_path %>
<%=form_tag "/teams/import_teams" do%>
  <%=file_field_tag :file%>
  <%= submit_tag "importar"%>
<%end%>
